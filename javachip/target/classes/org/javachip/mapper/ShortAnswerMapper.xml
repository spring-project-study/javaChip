<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.javachip.mapper.ShortAnswerMapper">

	<insert id="insert" useGeneratedKeys="true" keyProperty="answer_id">
		INSERT INTO tbl_short_answer (problem_id, answer_content) 
		VALUES (#{problem_id}, #{answer_content})
	</insert>
	
</mapper>